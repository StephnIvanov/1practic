#1
import math
x = 14.26
y = -1.22
z = 3.5 * 10**-2
a = 2*(math.cos(x-2/3))
b = 0.5+((math.sin(y)**2))
c = z**2/3-z**2/5+1
s = a/b*c
print(s)
#2
import math
x = -4.5
y = 0.75*10**-4
z = -0.845*10**2
a = (9+(x-y)**2)**(1/3)
b = x**2+y**2+2
c = math.exp(abs(x-y))*((math.tan(z))**3)
s = a/b-c
print(s)
#8
import cmath
q = -2.235 * 10**-2
w = 2.23
e = 15.221
a = (cmath.exp(abs(q-w))) * ((abs(q-w))**(q+w))
b = cmath.atan(q) + cmath.atan(e)
c = (q**6 + cmath.log10(w)**2)**(1/3)
s = a/b + c
print(s)
